{
  "provider": {
    "active": "github_models",
    "options": {
      "github_models": {
        "base_url": "https://models.inference.ai.azure.com",
        "model": "gpt-5-mini",
        "api_key_env": "GITHUB_TOKEN"
      },
      "openai": {
        "base_url": "https://api.openai.com/v1",
        "model": "gpt-5-mini",
        "api_key_env": "OPENAI_API_KEY"
      },
      "ollama": {
        "base_url": "http://localhost:11434/v1",
        "model": "llama3.1:8b",
        "api_key_env": ""
      },
      "lmstudio": {
        "base_url": "http://localhost:1234/v1",
        "model": "local-model",
        "api_key_env": ""
      }
    }
  },
  "copilot": {
    "connected": false,
    "github_login": null,
    "token_env": "GITHUB_TOKEN"
  },
  "security": {
    "sandbox_mode": true,
    "tailnet_only": false,
    "tailscale_cidrs": [
      "100.64.0.0/10"
    ],
    "tailscale_node_allowlist": [],
    "allowed_paths": [
      "/Users/tito1/Desktop/Test"
    ]
  },
  "agents": {
    "max_active": 4
  },
  "pipelines": {
    "mode": "sequential",
    "max_retries": 1
  },
  "bus": {
    "backend": "local",
    "redis_url": "redis://localhost:6379/0",
    "stream_key": "ontoti:bus",
    "max_messages": 1000
  }
}
